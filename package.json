{
    "name": "decyphra-website",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "node scripts/dev.js",
        "build": "node scripts/build.js",
        "start": "next start",
        "lint": "next lint",
        "type-check": "tsc --noEmit",
        "db:migrate": "npx dotenv-cli -e .env.local -- prisma migrate dev",
        "db:push": "npx dotenv-cli -e .env.local -- prisma db push",
        "db:seed": "npx dotenv-cli -e .env.local -- tsx prisma/seed.ts",
        "test:connection": "node scripts/test-connection.js",
        "db:studio": "npx dotenv-cli -e .env.local -- prisma studio",
        "db:generate": "prisma generate",
        "postinstall": "prisma generate"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.9.0",
        "@prisma/client": "^5.19.0",
        "@react-three/drei": "^9.122.0",
        "@react-three/fiber": "^8.18.0",
        "clsx": "^2.1.1",
        "date-fns": "^3.6.0",
        "face-api.js": "^0.22.2",
        "framer-motion": "^11.3.0",
        "gsap": "^3.12.5",
        "lucide-react": "^0.556.0",
        "next": "^14.2.0",
        "ogl": "^1.0.11",
        "postprocessing": "^6.38.0",
        "react": "^18.3.0",
        "react-dom": "^18.3.0",
        "react-hook-form": "^7.52.0",
        "three": "^0.181.2",
        "zod": "^3.23.0"
    },
    "prisma": {
        "seed": "npx dotenv-cli -e .env.local -- tsx prisma/seed.ts"
    },
    "devDependencies": {
        "@types/node": "^20.14.0",
        "@types/react": "^18.3.0",
        "@types/react-dom": "^18.3.0",
        "@types/three": "^0.181.0",
        "autoprefixer": "^10.4.0",
        "dotenv-cli": "^7.4.2",
        "eslint": "^8.57.0",
        "eslint-config-next": "^14.2.0",
        "postcss": "^8.4.0",
        "prettier": "^3.3.0",
        "prettier-plugin-tailwindcss": "^0.6.0",
        "prisma": "^5.19.0",
        "tailwindcss": "^3.4.0",
        "tsx": "^4.16.0",
        "typescript": "^5.5.0"
    }
}
